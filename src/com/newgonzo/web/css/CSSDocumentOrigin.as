package com.newgonzo.web.css {	import org.w3c.css.sac.CSSError;		public class CSSDocumentOrigin 	{		// ordered by priority		public static const USER_AGENT:int = 0;		public static const USER:int = 1;		public static const AUTHOR:int = 2;				public static function compare(origin1:int, origin2:int):int		{			return origin1 == origin2 ? -1 : origin1 > origin2 ? origin1 : origin2;		}				public static function toString(origin:int):String		{			switch(origin)			{				case USER_AGENT: return "USER_AGENT";				case USER: return "USER";				case AUTHOR: return "AUTHOR";			}						throw new CSSError("Unknown origin " + origin);		}			}}