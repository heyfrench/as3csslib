package com.newgonzo.commons.logging.targets{	import com.newgonzo.commons.logging.*;	import com.newgonzo.commons.logging.targets.display.ILogWindowDisplay;		public class LogWindowTarget extends AbstractTarget	{		private var window:ILogWindowDisplay;				public function LogWindowTarget(window:ILogWindowDisplay):void		{			this.window =  window;		}				public override function logEvent(event:LogEvent):void		{			window.onLog(event);		}	}}